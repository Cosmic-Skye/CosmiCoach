<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>ITSAppUsesNonExemptEncryption</key>
	<false/>
	<key>UIApplicationSceneManifest</key>
	<dict>
		<key>UIApplicationSupportsMultipleScenes</key>
		<false/>
	</dict>
	<key>NSBonjourServices</key>
	<array>
		<string>_cosmiccoach._tcp</string>
		<string>_cosmiccoach._udp</string>
	</array>
	<key>NSLocalNetworkUsageDescription</key>
	<string>CosmiCoach needs to access your local network to sync chat history and memory with across your devices.</string>
	<key>NSSpeechRecognitionUsageDescription</key>
	<string>CosmiCoach needs access to speech recognition so you can dictate messages.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>CosmiCoach needs microphone access to enable dictation for messages.</string>
</dict>
</plist>
